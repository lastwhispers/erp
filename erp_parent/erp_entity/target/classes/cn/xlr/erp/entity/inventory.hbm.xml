<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.xlr.erp.entity.Inventory" table="inventory" >
        <id name="uuid" >
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"> <param name="sequence_name">inventory_seq</param> </generator>
        </id>

        <property name="goodsuuid" /> 
        <property name="storeuuid" /> 
        <property name="num" /> 
        <property name="type" /> 
        <property name="createtime" /> 
        <property name="checktime" /> 
        <property name="creater" /> 
        <property name="checker" /> 
        <property name="state" /> 
        <property name="remark" /> 
 
    </class>
</hibernate-mapping>
